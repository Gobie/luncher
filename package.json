{
  "name": "luncher",
  "version": "0.0.1",
  "description": "Accessible lunch menus",
  "main": "server.js",
  "scripts": {
    "build:prod": "./node_modules/.bin/webpack --display-error-details --config webpack.config.prod.js",
    "build:dev": "./node_modules/.bin/webpack-dev-server --display-error-details --config webpack.config.dev.js",
    "coverage:check": "./node_modules/.bin/babel-istanbul check-coverage --functions 90",
    "coverage:ci": "cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage",
    "coverage:generate": "./node_modules/.bin/babel-istanbul cover ./node_modules/.bin/_mocha -- -R spec --bail --check-leaks test/**/*.spec.js",
    "lint": "./node_modules/.bin/eslint lib server service client test",
    "service": "node service.js",
    "server": "node server.js",
    "test": "npm run lint && npm run coverage:generate && npm run coverage:check",
    "test:ci": "npm run lint && npm run coverage:generate && npm run coverage:ci && npm run coverage:check",
    "test:mocha": "./node_modules/.bin/mocha -R spec --bail --check-leaks --compilers js:babel-register test/**/*.spec.js"
  },
  "engines": {
    "node": "5.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Gobie/luncher.git"
  },
  "keywords": [
    "lunch"
  ],
  "author": "Michal Bra≈°na <michal.brasna@gmail.com> (https://github.com/Gobie)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gobie/luncher/issues"
  },
  "homepage": "https://github.com/Gobie/luncher#readme",
  "dependencies": {
    "@slack/client": "^2.3.0",
    "amqp-connection-manager": "^1.3.1",
    "amqplib": "^0.4.1",
    "async": "^1.5.2",
    "compression": "^1.6.1",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "helmet": "^1.3.0",
    "lodash": "^4.9.0",
    "memjs": "^0.9.0",
    "moment": "^2.12.0",
    "morgan": "^1.7.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "request": "^2.70.0",
    "throng": "^3.0.0",
    "tv4": "^1.2.7",
    "uuid": "^2.0.1",
    "ware": "^1.3.0",
    "winston": "^2.2.0",
    "x-ray": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-istanbul": "^0.7.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.7.2",
    "codacy-coverage": "^1.1.3",
    "eslint": "^2.7.0",
    "eslint-plugin-react": "^4.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "mocha": "^2.4.5",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
