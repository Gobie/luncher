{
  "name": "luncher",
  "version": "0.0.1",
  "description": "Lunches nearby",
  "main": "server.js",
  "scripts": {
    "check-coverage": "istanbul check-coverage --functions 100 --statements 100 --branches 100 --lines 100",
    "coverage": "istanbul cover _mocha -- -R spec --bail --check-leaks test/**/*.spec.js",
    "lint": "xo",
    "service": "node service.js",
    "start": "node server.js",
    "test": "npm run lint && npm run coverage && npm run check-coverage"
  },
  "engines": {
    "node": "5.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Gobie/luncher.git"
  },
  "keywords": [
    "lunch"
  ],
  "author": "Michal Bra≈°na <michal.brasna@gmail.com> (https://github.com/Gobie)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gobie/luncher/issues"
  },
  "homepage": "https://github.com/Gobie/luncher#readme",
  "dependencies": {
    "amqp-connection-manager": "^1.3.1",
    "amqplib": "^0.4.1",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "helmet": "^1.1.0",
    "lodash": "^4.5.1",
    "throng": "^2.0.1",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "xo": "^0.12.1"
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "envs": [
      "node",
      "mocha"
    ]
  }
}
